<!-- slide 1 -->
	<component id='ID_s1_title'>
		<text><![CDATA[<b>Decimal Positional Notation</b>]]></text>
	</component>

<!--end slide 1 table-->

	<!-- slide 1 table for flash-->
	<component id='ID_s1_table'>
