<text><![CDATA[<b>Les différents types d'adresse d'un réseau</b>]]></text>
</component>

<component id="ID_s1_txt01">
  <text><![CDATA[<b>Réseau</b>]]></text> 
  </component>
  
  <component id="ID_s1_txt02">
  <text><![CDATA[Adresse réseau]]></text> 
  </component>
  
<component id="ID_s1_txt03">
  <text><![CDATA[<b>Adresses d'hôte</b>]]></text> 
  </component>
  
  <component id="ID_s1_txt04">
