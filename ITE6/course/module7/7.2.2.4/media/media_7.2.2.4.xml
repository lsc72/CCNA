<textcomponents>
  <component id="ID_title">
    <text><![CDATA[<b>Comparaison des normes 802.11</b>]]></text>
  </component>
  <component id="ID_table">
    <text><![CDATA[<table header='none' bordercolor='#F2F2F2' borderwidth='3' shadow='false' shadowcolor='#000000' glow='true' glowcolor='#808080' cellpadding='4'>
		<tr class="mtable_title">
			<td colspan="5" bgcolor='#F2F2F2'><font size="16"></font></td>
		</tr>
		<tr class="mtable_top">
			<td colspan='5' bgcolor='#4E4C4D'><font size='1' color='#4E4C4D'>.</font></td>
		</tr>
	<tr>
		<td bgcolor='#666263'  valign="top" width='95'><font color='#F2F2F2' size="11"><b>Norme IEEE</b></font></td>
		<td bgcolor='#666263'  valign="top" ><font color='#F2F2F2' size="11"><b>Débit maximal</b></font></td>
		<td bgcolor='#666263'  valign="top" ><font color='#F2F2F2' size="11"><b>Portée maximale à l'intérieur</b></font></td>
		<td bgcolor='#666263'  valign="top" ><font color='#F2F2F2' size="11"><b>Fréquence</b></font></td>
		<td bgcolor='#666263'  valign="top" ><font color='#F2F2F2' size="11"><b>Rétrocompatibilité</b></font></td>
	</tr>

	<tr>
		<td bgcolor='#E9E7E8' valign="top">802.11a</td>
		<td bgcolor='#E9E7E8' valign="top">54 Mbit/s</td>
		<td bgcolor='#E9E7E8' valign="top">35 m</td>
		<td bgcolor='#E9E7E8' valign="top">5 GHz</td>
		<td bgcolor='#E9E7E8' valign="top">—</td>
		
	</tr>
	
	
	<tr>
		<td bgcolor='#F2F2F2' valign="top">802.11b</td>
		<td bgcolor='#F2F2F2' valign="top">11 Mbit/s</td>
		<td bgcolor='#F2F2F2' valign="top">35 m</td>
		<td bgcolor='#F2F2F2' valign="top">2,4 GHz</td>
		<td bgcolor='#F2F2F2' valign="top">—</td>
		
		
	</tr>

	   <tr>
		<td bgcolor='#E9E7E8' valign="top">802.11g</td>
		<td bgcolor='#E9E7E8' valign="top">54 Mbit/s</td>
		<td bgcolor='#E9E7E8' valign="top">38 m</td>
		<td bgcolor='#E9E7E8' valign="top">2,4 GHz</td>
		<td bgcolor='#E9E7E8' valign="top">802.11b</td>
		
	</tr>
	
	<tr>
		<td bgcolor='#F2F2F2' valign="top">802.11n</td>
		<td bgcolor='#F2F2F2' valign="top">600 Mbit/s</td>
		<td bgcolor='#F2F2F2' valign="top">70 m</td>
		<td bgcolor='#F2F2F2' valign="top">2,4 GHz et 5 GHz</td>
		<td bgcolor='#F2F2F2' valign="top">802.11a/b/g</td>
		
		
	</tr>

	   <tr>
		<td bgcolor='#E9E7E8' valign="top">802.11ac</td>
		<td bgcolor='#E9E7E8' valign="top">1,3 Gbit/s (1 300 Mbit/s)</td>
		<td bgcolor='#E9E7E8' valign="top">35 m</td>
		<td bgcolor='#E9E7E8' valign="top">5 GHz</td>
		<td bgcolor='#E9E7E8' valign="top">802.11a/n</td>
		
	</tr>
	
	</table>]]></text>
  </component>
